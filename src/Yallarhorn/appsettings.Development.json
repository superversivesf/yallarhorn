{
  "Version": "1.0",
  "PollInterval": 300,
  "MaxConcurrentDownloads": 2,

  "Server": {
    "Host": "0.0.0.0",
    "Port": 5001,
    "BaseUrl": "http://localhost:5001",
    "FeedPath": "/feeds",
    "UseHttps": false,
    "MaxConcurrentConnections": 50,
    "RequestTimeoutSeconds": 60
  },

  "Database": {
    "Path": "./data/yallarhorn-dev.db",
    "PoolSize": 3
  },

  "TranscodeSettings": {
    "AudioFormat": "mp3",
    "AudioBitrate": "128k",
    "AudioSampleRate": 44100,
    "VideoFormat": "mp4",
    "VideoCodec": "h264",
    "VideoQuality": 28,
    "Threads": 2,
    "KeepOriginal": false
  },

  "Auth": {
    "FeedCredentials": {
      "Enabled": false,
      "Realm": "Yallarhorn Feeds (Development)"
    },
    "AdminAuth": {
      "Enabled": false
    }
  },

  "Workers": {
    "Enabled": true,
    "RefreshIntervalSeconds": 300,
    "DownloadPollIntervalSeconds": 3
  },

  "Channels": [],

  "LoggingOptions": {
    "MinimumLevel": "Debug",
    "EnableConsole": true,
    "EnableFile": true,
    "FilePath": "logs/yallarhorn-development-.log",
    "RollingInterval": "Day",
    "RetainedFileCount": 7
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "System": "Information"
    }
  }
}